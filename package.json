{
  "name": "methods-run",
  "version": "1.0.0",
  "description": "Es una pequeña recopilación de formas para hacer arrancar [Webpck](webpack.github.io).",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "better-npm-run build"
  },
  "betterScripts": {
    "build": {
      "command": "node_modules/.bin/webpack --config webpack.build",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "PORT": 8080,
        "HOST": "localhost"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kasselTrankos/webpack-method-start.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kasselTrankos/webpack-method-start/issues"
  },
  "homepage": "https://github.com/kasselTrankos/webpack-method-start#readme",
  "dependencies": {
    "webpack": "^1.12.10",
    "webpack-dev-server": "^1.14.0"
  },
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-core": "^6.4.0",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "better-npm-run": "0.0.5"
  }
}
